<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="partner_state_area_form" model="ir.ui.view">
            <field name="name">partner_state_area_view.form</field>
            <field name="model">res.partner</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
				<xpath expr="//form/sheet/group[1]/group[1]/field[@name='website']" position="after">
					<field string="State Area" name="state_area"/>
				</xpath>
            </field>
        </record>
    </data>
</openerp>
